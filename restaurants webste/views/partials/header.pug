div#header 
    ul 
        li 
            a(href='/') Home
        li 
            a(href='/users') Users

        if (session.loggedIn)
            li 
                a(href='/orderform') Order
            li 
                a(href='/users/' + session.userId) Profile
            li 
                a(href='/logout') Logout
        else
            li 
                a(href='/login') Login
            li 
                a(href='/register') Register
        